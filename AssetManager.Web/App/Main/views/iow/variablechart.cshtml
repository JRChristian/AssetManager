<div ng-controller="app.views.variable.chart as vm">
    <div class="container">
        <div class="row">
            <div class="col-sm-9">
                <h3>{{vm.name}} <small> {{vm.description}}</small></h3>
            </div>
            <div class="col-sm-3">
                <a ui-sref="iowvariableview({ Id: {{vm.variable.id}} })" class="btn btn-primary btn-sm"><i class="fa fa-binoculars"></i> @L("View")</a>
                <a ui-sref="iowvariableedit({ Id: {{vm.variable.id}} })" class="btn btn-default btn-sm" disabled="disabled"><i class="fa fa-image"></i> @L("Drawings")</a>
                <a ui-sref="iowvariableedit({ Id: {{vm.variable.id}} })" class="btn btn-default btn-sm"><i class="fa fa-wrench"></i> @L("Edit")</a>
            </div>
            <div class="row">
                <div class="col-xs-12">
                    <div id="chartContainer" style="width:100%; height:400px;"></div>
                </div>
            </div>
        </div>
    </div>
