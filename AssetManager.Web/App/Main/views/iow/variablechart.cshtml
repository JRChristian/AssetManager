<div ng-controller="app.views.variable.chart as vm">
    <div class="container">
        <div class="row">
                <h3>{{vm.name}} <small> {{vm.description}}</small></h3>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div id="chartContainer" style="width:100%; height:600px;"></div>
            </div>
        </div>
        <div class="row">
            <a ui-sref="iowvariablelist" class="btn btn-info btn-sm"><i class="fa fa-sitemap"></i> @L("IOWVariables")</a>
            <a ui-sref="iowlimitstatus" class="btn btn-info btn-sm"><i class="fa fa-sitemap"></i> @L("IowTtlLimitStatus")</a>
            <a ui-sref="iowvariabledeviations({ Id: {{vm.variable.id}} })" class="btn btn-info btn-sm"><i class="fa fa-exclamation-circle"></i> @L("IowBtnDeviations")</a>
            <a ui-sref="iowvariableview({ Id: {{vm.variable.id}} })" class="btn btn-info btn-sm"><i class="fa fa-binoculars"></i> @L("View")</a>
            <a ui-sref="iowvariableedit({ Id: {{vm.variable.id}} })" class="btn btn-info btn-sm" disabled="disabled"><i class="fa fa-image"></i> @L("Drawings")</a>
            <a ui-sref="iowvariableedit({ Id: {{vm.variable.id}} })" class="btn btn-success btn-sm"><i class="fa fa-pencil-square-o"></i> @L("Edit")</a>
        </div>
    </div>
</div>
